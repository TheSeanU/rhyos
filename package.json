{
  "name": "vite-svg-icons",
  "version": "0.0.1",
  "private": true,
  "description": "svg icons components auto importing for Vue",
  "author": "rhyos <sstwunett@gmail.com>",
  "license": "MIT",
  "funding": "https://rhyos.nl",
  "homepage": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "engines": {
    "node": ">=16"
  },
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsc && ts-node src/index.ts",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@babel/parser": "^7.23.0",
    "vue": "3"
  },
  "peerDependenciesMeta": {
    "@babel/parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.4",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.1",
    "resolve": "^1.22.4",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.9",
    "@types/node": "^20.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue": "^3.2.36"
  }
}
