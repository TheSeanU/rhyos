    "@vitejs/plugin-vue": "^4.2.3",
    "vite-plugin-windicss": "^1.8.10",
    "windicss": "^3.5.6",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.3"
  }
}
